	<!--<h1>Listing weeklyadds</h1>-->
<div class= "column span-24">
	
 	<div class = "column span-10 append-2 box">
		<table>
 		<th><h3>All Weekly Adds Links</h3></th>
 		    <col style="width:380px" />    
 		    <col style="width:5px" />
 		    <col style="width:5px" />
		<% @weeklyadds.each do |weeklyadd| %>
			<tr class= "<%= cycle("rowA", "rowB")%>">
				<td><%= weeklyadd.name %></td>  
				<td><%= weeklyadd.category_id %></td>
				<td><%=link_to(image_tag("ButtonLetsgo.png", :size => "45x20", :border => 0), url_with_protocol(weeklyadd.url),:target => :blank)%></td>
			</tr>
		<% end %>
		</table>
	</div>
	
	<div class = "column span-10 last box">
		<h3>Home of future avertisements.</h3>
	</div>
</div>
<%= link_to 'New Weeklyadd', new_weeklyadd_path %>